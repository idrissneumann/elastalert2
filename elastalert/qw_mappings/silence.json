{
  "doc_mapping": {
    "dynamic_mapping": {
      "description": "silenced alerts from elastalert2",
      "expand_dots": true,
      "fast": true,
      "indexed": true,
      "record": "basic",
      "stored": true,
      "tokenizer": "raw"
    },
    "field_mappings": [
      {
        "name": "rule_name",
        "type": "text"
      },
      {
        "name": "until",
        "type": "datetime"
      },
      {
        "name": "@timestamp",
        "type": "datetime",
        "fast": true
      }
    ],
    "store_source": true,
    "timestamp_field": "@timestamp"
  },
  "index_id": "silence",
  "search_settings": {
    "default_search_fields": [
      "rule_name", 
      "until",
      "@timestamp"
    ]
  },
  "version": "0.8"
}
